@prefix rml: <http://w3id.org/rml/> .
 [
    a rml:TriplesMap;
    rml:logicalSource [
      rml:source "data.json" ;
      rml:referenceFormulation rml:JSONPath ;
      rml:iterator "$[*]";
    ];
    rml:subjectMap [
      rml:template "https://example.org/instances/{id}";
    ];
    rml:predicateObjectMap [
      rml:predicate <http://example.org/ns/p> ;
      rml:object "train"@en ;
    ];
  ] .
